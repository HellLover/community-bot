{
    "compilerOptions": {
      "target": "ESNEXT",
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "strictPropertyInitialization": false,
      "skipLibCheck": false,
      "inlineSourceMap": true,
      "noImplicitAny": false,
      "allowJs": true,
      "checkJs": true
    }
  }